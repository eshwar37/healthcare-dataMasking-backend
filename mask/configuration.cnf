		    <entity>
            <entity_name>COUNTRY</entity_name>
            <original_name>COUNTRY</original_name>
            <masking_type>Redact</masking_type>
        </entity>
        <entity>
            <entity_name>STATE</entity_name>
            <original_name>STATE</original_name>
            <masking_type>Redact</masking_type>
        </entity>
        <entity>
            <entity_name>LOCATION</entity_name>
            <original_name>LOCATION</original_name>
            <masking_type>Redact</masking_type>
        </entity>
    </plugins>
    <dataset>
        <dataset_location>data/data/mask_input/unnotated_texts_nerm/brat/deploy</dataset_location>
        <data_output>data/output</data_output>
    </dataset>
