            <masking_type>Redact</masking_type>
        </entity>
        <entity>
            <entity_name>DOCTOR</entity_name>
            <original_name>DOCTOR</original_name>
            <masking_type>Mask</masking_type>
            <masking_class>MaskFirstNameRandomized</masking_class>
        </entity>
        <entity>
            <entity_name>MEDICALRECORD</entity_name>
            <original_name>MEDICALRECORD</original_name>
            <masking_type>Mask</masking_type>
