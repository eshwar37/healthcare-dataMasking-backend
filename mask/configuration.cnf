        <entity>
            <entity_name>DATE</entity_name>
            <original_name>DATE</original_name>
            <masking_type>Mask</masking_type>
            <masking_class>Mask_date_simple</masking_class>
        </entity>
		    <entity>
            <entity_name>PROFESSION</entity_name>
            <original_name>PROFESSION</original_name>
            <masking_type>Mask</masking_type>
            <masking_class>MaskJobRandomized</masking_class>
